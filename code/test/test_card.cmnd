! 1) Settings that will be used in a main program.
Main:numberOfEvents = 5000          ! number of events to generate
Next:numberShowEvent = 0           ! suppress full listing of first events

# random seed
Random:setSeed = on
Random:seed = 0

! 2) Beam parameter settings.
Beams:idA = 11                ! first beam,  e- = 11
Beams:idB = -11                ! second beam, e+ = -11
Beams:eCM = 91.2               ! CM energy of collision



# Pythia 8 settings for LEP
# Hadronic decays including b quarks, with ISR photons switched off
WeakSingleBoson:ffbar2gmZ = on
23:onMode = off
23:onIfAny = 1 2 3 4 5
PDF:lepton = off
SpaceShower:QEDshowerByL = off

StringZ:aLund = 0.8907313813100579

StringZ:bLund = 1.7739212819523444

StringZ:rFactC = 1.32
StringZ:rFactB = 0.855
StringZ:aExtraSQuark = 0.1
StringZ:aExtraDiquark=0.93
StringPT:sigma=0.336
StringPT:enhancedFraction=0.1
StringPT:enhancedWidth=2.0
StringFlav:ProbStoUD=0.217
StringFlav:probQQtoQ=0.3
StringFlav:probSQtoQQ=0.92
StringFlav:ProbQQ1toQQ0=0.2
TimeShower:alphaSvalue=0.14
TimeShower:pTmin=0.7


